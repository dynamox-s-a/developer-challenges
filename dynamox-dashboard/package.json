{
  "name": "dynamox-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/sync-data.js && vite",
    "dev:json-server": "cross-env VITE_USE_JSON_SERVER=true node scripts/sync-data.js && vite",
    "build": "node scripts/sync-data.js && tsc -b && vite build",
    "build:vercel": "node scripts/sync-data.js && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sync-data": "node scripts/sync-data.js",
    "json-server": "json-server --watch data/response-challenge-v2.json --port 3001",
    "json-server:routes": "json-server --watch data/response-challenge-v2.json --port 3001",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:all": "npm run test:run && npm run test:e2e"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.12.2",
    "cross-env": "^7.0.3",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "redux-saga": "^1.3.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "cypress": "^15.3.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "json-server": "^1.0.0-beta.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
